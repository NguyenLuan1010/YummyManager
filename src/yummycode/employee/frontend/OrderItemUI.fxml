<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER_LEFT" onMousePressed="#onclickFoodItem" prefHeight="65.0" prefWidth="350.0" spacing="5.0" style="-fx-border-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yummycode.employee.controller.ordercontroller.OrderItem_controller">
   <children>
      <Label fx:id="nameFood" alignment="CENTER" prefHeight="45.0" prefWidth="183.0" text="Name Food">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="quantity" alignment="CENTER" layoutX="10.0" layoutY="20.0" prefHeight="45.0" prefWidth="110.0" text="10">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </Label>
      <Button layoutX="137.0" layoutY="27.0" mnemonicParsing="false" onAction="#onclickMinus" prefHeight="31.0" prefWidth="100.0" text="-">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#onclickAdd" prefHeight="31.0" prefWidth="100.0" text="+">
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </Button>
      <Label fx:id="totalMoney" alignment="CENTER" layoutX="202.0" layoutY="20.0" prefHeight="45.0" prefWidth="132.0" text="188">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
   </children>
</HBox>
